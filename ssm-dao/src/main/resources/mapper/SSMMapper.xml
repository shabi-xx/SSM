<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cn.itsource.dao.SSMMapper">
    <insert id="insert" parameterType="ssm">
        insert into ssm values(#{id},#{name},#{birthday})
    </insert>


    <select id="queryAll" resultType="cn.itsource.entity.SSM">
        select * FROM  ssm
    </select>
</mapper>